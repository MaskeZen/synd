{
  "name": "synd",
  "version": "0.0.9",
  "description": "yet another rsync wrapper",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "rm -rf ./dist && tsc",
    "lint": "eslint src test --ext=.js,.ts",
    "preversion": "npm run test && npm run build",
    "postversion": "npm publish && git push --follow-tags",
    "types": "tsc --noEmit",
    "types:watch": "tsc --noEmit --watch",
    "coveralls": "npm run test -- --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "bin": {
    "synd": "./bin/synd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonk52/synd.git"
  },
  "keywords": [
    "rsync",
    "cli"
  ],
  "author": "antonk52",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antonk52/synd/issues"
  },
  "homepage": "https://github.com/antonk52/synd#readme",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "commander": "^3.0.0",
    "kleur": "^3.0.3",
    "lodash.debounce": "^4.0.8",
    "rsync": "^0.6.1"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^13.9.8",
    "@types/rimraf": "^3.0.0",
    "@types/rsync": "^0.4.30",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "coveralls": "^3.0.6",
    "directory-tree": "^2.2.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3"
  }
}
